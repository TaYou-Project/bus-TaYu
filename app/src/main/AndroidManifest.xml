<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.devTayu.busTayu">

    <!-- Permission  -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"

        android:icon="@mipmap/ic_launcher_bus"
        android:roundIcon="@mipmap/ic_launcher_bus_round"

        android:supportsRtl="true"
        android:theme="@style/Theme.BusTAYU"
        android:networkSecurityConfig="@xml/network_security_config"
        android:usesCleartextTraffic="true">

        <!-- APP KEY -->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="a697f737083608190f828c2820f276ee"/>

        <activity
            android:name="org.devTayu.busTayu.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="org.devTayu.busTayu.activity.SetupActivity"/>
        <activity android:name="org.devTayu.busTayu.activity.LikedActivity"/>
        <activity android:name="org.devTayu.busTayu.activity.ReserveActivity"/>
        <activity android:name="org.devTayu.busTayu.activity.AroundActivity"/>
        <activity android:name="org.devTayu.busTayu.activity.SearchActivity"/>
        <activity android:name="org.devTayu.busTayu.activity.RouteActivity" />

    </application>

</manifest>